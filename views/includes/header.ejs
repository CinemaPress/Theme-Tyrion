<div style="height:432px;">

    <a href="/" class="logov2_1">Главная</a>

    <div class="searchv2_1">
        <div class="searchv2_1_back">
            <form method="get" name="searchform" action="/<%- page.urls.search %>/title">
                <input id="story" class="searchv2_2" name="q" value="Введите ваш текст для поиска..." onblur="if(this.value=='') this.value='Введите ваш текст для поиска...';" onfocus="if(this.value=='Введите ваш текст для поиска...') this.value='';" type="text"/>
                <input type="image" class="searchv2_3" src="/themes/tyrion/public/images/spacer.gif?1" alt=""/>
            </form>
        </div>
    </div>

    <div class="loginv2_1">

        <div class="loginv2_2 continueViewingBlock" style="display: none;">
            <p class="link">
                <img src="/themes/tyrion/public/images/new/btn_trailer.png" style="margin: 5px 5px 0 0; width: 10px;" alt="Продолжить просмотр?">
                <a href="javascript:void(0)" id="continueViewing">
                    Продолжить просмотр?
                </a>
            </p>
        </div>

    </div>

    <div class="hmenuv2_1">
        <a href="/" class="btnHome"></a>
        <ul>
            <li>
                <a href="/<%- page.urls.type %>/<%- page.urls.types.movie %>">Фильмы</a>
            </li>

            <li>
                <a href="/<%- page.urls.type %>/<%- page.urls.types.mult %>">Мультфильмы</a>
            </li>
            <li>
                <a href="/<%- page.urls.type %>/<%- page.urls.types.serial %>">Сериалы</a>
            </li>
            <li>
                <a href="/<%- page.urls.type %>/<%- page.urls.types.anime %>">Аниме</a>
            </li>
            <li>
                <a href="/<%- page.urls.type %>/<%- page.urls.types.tv %>">Передачи</a>
            </li>
            <li><a href="#">ТОП 500</a>
                <ul>
                    <li><a href="/<%- page.urls.type %>/<%- page.urls.types.movie %>?sorting=kinopoisk-rating-up">По рейтингу КиноПоиск</a></li>
                    <li><a href="/<%- page.urls.type %>/<%- page.urls.types.movie %>?sorting=imdb-rating-up">По рейтингу IMDb</a></li>
                    <li><a href="/<%- page.urls.type %>/<%- page.urls.types.movie %>?sorting=kinopoisk-vote-up">По голосам КиноПоиск</a></li>
                    <li><a href="/<%- page.urls.type %>/<%- page.urls.types.movie %>?sorting=imdb-vote-up">По голосам IMDb</a></li>
                </ul>
            </li>
        </ul>

        <% if (page.social) { %>

        <div class="socbox">
            <% if (page.social.vk) { %>
            <a class="ftrsocbox_item soch-vk" href="<%- page.social.vk %>" target="_blank"></a>
            <% } %>
            <% if (page.social.facebook) { %>
            <a class="ftrsocbox_item soch-fb" href="<%- page.social.facebook %>" target="_blank"></a>
            <% } %>
            <% if (page.social.youtube) { %>
            <a class="ftrsocbox_item soch-yt" href="<%- page.social.youtube %>" target="_blank"></a>
            <% } %>
            <% if (page.social.instagram) { %>
            <a class="ftrsocbox_item soch-ig" href="<%- page.social.instagram %>" target="_blank"></a>
            <% } %>
            <% if (page.social.twitter) { %>
            <a class="ftrsocbox_item soch-od" href="<%- page.social.twitter %>" target="_blank"></a>
            <% } %>
            <% if (page.social.gplus) { %>
            <a class="ftrsocbox_item soch-tw" href="<%- page.social.gplus %>" target="_blank"></a>
            <% } %>
        </div>

        <% } %>

    </div>

    <% if (typeof slider === 'object' && slider.length) { %>

    <div class="bf_top_films">

        <div class="bf_top_films_bl">
            <div class="bf_top_films_bl_title_back bf_top_films_bl_title bf_top_films_bl_title_back_1">
                <div class="bf_top_films_bl_title_back_ic bf_top_films_bl_title_back_ic_1"></div>
                ТОП фильмы
                <a href="/<%- page.urls.type %>/<%- page.urls.types.movie %>" class="bf_top_films_bl_li_more bf_top_films_bl_li_more_1" rel="nofollow">Показать еще</a>
            </div>
            <div class="bf_top_films_bl_li bf_top_films_bl_li_1">
                <ul>
                    <% slider.slice(0,6).forEach(function (movie) { %>
                    <li>
                        <a href="<%- movie.url %>"><% if (movie.title.length >= 35) { %><%- movie.title.substr(0, 35).trim() %>... <%- movie.year %><% } else { %><%- movie.title %> <%- movie.year %><% } %></a>
                    </li>
                    <% }); %>
                </ul>
            </div>
        </div>

        <div class="bf_top_films_bl">
            <div class="bf_top_films_bl_title_back bf_top_films_bl_title bf_top_films_bl_title_back_2">
                <div class="bf_top_films_bl_title_back_ic bf_top_films_bl_title_back_ic_2"></div>
                Популярные
                <a href="/<%- page.urls.type %>/<%- page.urls.types.movie %>?sorting=imdb-vote-up" class="bf_top_films_bl_li_more bf_top_films_bl_li_more_2" rel="nofollow">Показать еще</a>
            </div>
            <div class="bf_top_films_bl_li bf_top_films_bl_li_2">
                <ul class="bf_top_films_bl_li_2">
                    <% slider.slice(6,12).forEach(function (movie) { %>
                    <li>
                        <a href="<%- movie.url %>"><% if (movie.title.length >= 35) { %><%- movie.title.substr(0, 35).trim() %>... <%- movie.year %><% } else { %><%- movie.title %> <%- movie.year %><% } %></a>
                    </li>
                    <% }); %>
                </ul>
            </div>
        </div>

        <div class="bf_top_films_bl bf_top_films_bl_last">
            <div class="bf_top_films_bl_title_back bf_top_films_bl_title bf_top_films_bl_title_back_3">
                <div class="bf_top_films_bl_title_back_ic bf_top_films_bl_title_back_ic_3"></div>
                Новинки в кино
                <% page.urls.years.slice(0,1).forEach(function(year) { %>
                <a href="<%- year.url %>" class="bf_top_films_bl_li_more bf_top_films_bl_li_more_3" rel="nofollow">Показать еще</a>
                <% }); %>
            </div>
            <div class="bf_top_films_bl_li bf_top_films_bl_li_3">
                <ul class="bf_top_films_bl_li_3">
                    <% slider.slice(12,18).forEach(function (movie) { %>
                    <li>
                        <a href="<%- movie.url %>"><% if (movie.title.length >= 35) { %><%- movie.title.substr(0, 35).trim() %>... <%- movie.year %><% } else { %><%- movie.title %> <%- movie.year %><% } %></a>
                    </li>
                    <% }); %>
                </ul>
            </div>
        </div>

    </div>

    <% } %>

</div>

<% if (typeof page.adv === 'object' && page.adv.top) { %>
<div style="clear: both;"></div>
<div style="text-align: center; margin: 10px auto">
    <%- page.adv.top %>
</div>
<div style="clear: both;"></div>
<% } %>