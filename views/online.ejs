<!doctype html>
<html lang="<%- page.language %>" prefix="og: http://ogp.me/ns# video: http://ogp.me/ns/video# ya: http://webmaster.yandex.ru/vocabularies/">
<head>

    <% include includes/head.ejs %>

</head>
<body class="class_body">

<div class="new_top_menu" id="new_top_menu">
    <div class="line">
        <div class="right">
            <div class="search">
                <form action="/<%- page.urls.search %>/title" name="searchform" method="get">
                    <input type="text" class="search_inp" name="q" autocomplete="off" placeholder="<%- page.l.search %>..."/>
                    <input type="submit" class="search_but" value="" rel="nofollow"/>
                </form>
            </div>
            <% page.urls.years.slice(0,1).forEach(function(year) { %>
            <div class="login">
                <a href="<%- year.url %>" rel="nofollow" class="new_films"></a>
            </div>
            <% }); %>
        </div>
    </div>
</div>

<div id="hv2_1">

    <div class="brand_back"></div>
    <div class="brand_back_wrap"></div>

    <div id="hv2_2">

        <div id="hv2_4">

            <div id="wrapper">

                <div id="header">

                    <% include includes/header.ejs %>

                </div>

                <div id="container">

                    <div class="con1 ">
                        <div class="con2">
                            <div class="con3">
                                <div class="con4">
                                    <div class="con5">

                                        <div id="left">

                                            <% include includes/left.ejs %>

                                        </div>

                                        <div id="center" class="aviable_showfull">

                                            <div class="bl_seo_txt_h bl_seo_txt_h_categ_sb">
                                                <div class="bl_seo_txt_bl_2">
                                                    <div class="tit_speedbar">
                                                        <span id="cinemapress-speedbar">
                                                            <span>
                                                                <a href="/" rel="nofollow">
                                                                    <%- page.l.home %>
                                                                </a>
                                                            </span>
                                                            <% if (movie.genre_url) { %>
                                                            <span class="separ"></span>
                                                            <span>
                                                                <%- movie.genre_url %>
                                                            </span>
                                                            <% } %>
                                                            <span class="separ"></span>
                                                            <span>
                                                                <a href="<%- movie.url %>">
                                                                    <%= movie.title %>
                                                                </a>
                                                            </span>
                                                            <span class="separ"></span>
                                                            <span>
                                                                <%- page.l.online %>
                                                            </span>
                                                        </span>
                                                    </div>
                                                </div>
                                            </div>

                                            <div id='cinemapress-content'>

                                                <div class="sh0">
                                                    <div class="sh1">
                                                        <div class="sh2">

                                                            <div class="sh17">
                                                                <div class="sh18">
                                                                    <%- movie.year %>
                                                                </div>
                                                            </div>

                                                            <h1><%- page.h1 %></h1>

                                                        </div>
                                                    </div>

                                                    <div class="sh15">

                                                        <div class="clearfix"></div>

                                                        <% if (page.seo) { %>
                                                        <div class="fulldisc">
                                                            <div style="display:inline;">
                                                                <%- page.seo %>
                                                            </div>
                                                        </div>
                                                        <% } %>

                                                        <div style="clear:both"></div>

                                                        <div class="episodesListBlock" style="display: none; margin: 10px auto">
                                                            <h3><%= movie.title %> <%- page.l.episodes %></h3>
                                                            <div id="episodesList" data-id="<%- movie.kp_id %>"></div>
                                                        </div>

                                                        <h2><%- page.l.online %> <%= movie.title %> <%- movie.year %></h2>

                                                        <br/>

                                                        <% if (typeof page.adv === 'object' && page.adv.over) { %>
                                                        <div style="text-align: center; margin: 10px auto">
                                                            <%- page.adv.over %>
                                                        </div>
                                                        <% } %>

                                                        <div class="section">
                                                            <ul class="tabz">
                                                                <li class="current">
                                                                    <%- page.l.online %>
                                                                </li>
                                                                <% if (page.urls.movies.trailer) { %>
                                                                <li>
                                                                    <a href="<%- movie.url %>/<%- page.urls.movies.trailer %>">
                                                                        <%- page.l.trailer %>
                                                                    </a>
                                                                </li>
                                                                <% } %>
                                                            </ul>
                                                            <div>
                                                                <div id="cinemapress-player" class="box visible" style="border-radius: 10px;">
                                                                    <% if (page.player) { %>
                                                                    <%- page.player %>
                                                                    <% } %>
                                                                </div>
                                                            </div>

                                                            <div class="line_rating_n">
                                                                <div class="watchLaterBlock" style="display: none;">
                                                                    <a href="javascript:void(0)" id="watchLater" class="watchlater">
                                                                        <%- page.l.later %>
                                                                    </a>
                                                                </div>
                                                                <div class="social-likes" data-counters="no">
                                                                    <div class="facebook" title="<%- page.l.facebook %>"></div>
                                                                    <div class="twitter" title="<%- page.l.twitter %>"></div>
                                                                    <div class="vkontakte" title="<%- page.l.vk %>"></div>
                                                                    <div class="plusone" title="<%- page.l.google %>"></div>
                                                                </div>
                                                            </div>

                                                            <div style="clear:both;"></div>

                                                            <% if (movie.pictures.length) { %>
                                                            <div style="margin: 10px auto; text-align: center;" id="lightgallery">
                                                                <% movie.pictures.forEach(function (picture) { %>
                                                                <% var ahref = page.urls.movies.picture
                                                                        ? movie.url + '/' + page.urls.movies.picture
                                                                        : picture.picture_big %>
                                                                <a href="<%- ahref %>"><img src="<%- picture.picture_min %>" alt="<%= movie.title %> <%- movie.year %> <%- page.l.picture %>" title="<%= movie.title %>"></a>
                                                                <% }); %>
                                                            </div>
                                                            <% if (!page.urls.movies.picture) { %>
                                                            <link href="/themes/default/public/desktop/css/lightgallery.min.css" rel="stylesheet">
                                                            <script src="/themes/default/public/desktop/js/lightgallery.min.js"></script>
                                                            <script>lightGallery(document.getElementById('lightgallery'));</script>
                                                            <% } %>
                                                            <% } %>

                                                            <div style="clear:both;"></div>

                                                            <% if (page.urls.movies.download) { %>
                                                            <div class="tor_down">
                                                                <a href="<%- movie.url %>/<%- page.urls.movies.download %>" rel="nofollow" class="bl_s_nh">
                                                                    <h3><%- page.l.download %></h3>
                                                                </a>
                                                                <div class="tor_title">
                                                                    <a href="<%- movie.url %>/<%- page.urls.movies.download %>">torrent</a>
                                                                </div>
                                                            </div>
                                                            <% } %>

                                                            <div style="clear:both;"></div>

                                                        </div>

                                                        <div class="clearfix"></div>

                                                        <% if (typeof page.adv === 'object' && page.adv.under) { %>
                                                        <div style="text-align: center; margin: 10px auto">
                                                            <%- page.adv.under %>
                                                        </div>
                                                        <% } %>

                                                        <% if (typeof contents === 'object' && contents.length) { %>
                                                        <div style="clear: both"></div>
                                                        <% contents.forEach(function (content) { %>
                                                        <div class="cinemapress-news-block">
                                                            <div style="margin: 0; padding: 10px">
                                                                <a href="<%- content.url %>" class="cinemapress-news" style="background-image:url(<%- content.image %>);">
                                                                    <div class="cinemapress-news-title">
                                                                        <%= content.title %>
                                                                    </div>
                                                                </a>
                                                            </div>
                                                        </div>
                                                        <% }); %>
                                                        <div style="clear: both"></div>
                                                        <% } %>

                                                    </div>

                                                </div>

                                                <% if (typeof movies === 'object') {
                                                        for (var type in movies) {
                                                if (movies.hasOwnProperty(type) && movies[type].length) {
                                                movies[type].forEach(function (category) {  %>
                                                <div class="relatnews_tit">
                                                    <div class="titlereln">
                                                        <h4>
                                                            <%- category.name %>
                                                        </h4>
                                                    </div>
                                                </div>
                                                <div class="relatnews">
                                                    <div class="slider1">
                                                        <% if (category.movies.length) {
                                                        category.movies.forEach(function (movie) {  %>
                                                        <div class="slide">
                                                            <div class="imgbox">
                                                                <div class="img">
                                                                    <a href="<%- movie.url %>" rel="nofollow">
                                                                        <img src="<%- movie.poster %>" alt="<%= movie.title %> <%- movie.year %>"/>
                                                                    </a>
                                                                </div>
                                                                <div class="reltexta">
                                                                    <a href="<%- movie.url %>"><%= movie.title %> <%- movie.year %></a>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <% }); } %>
                                                    </div>
                                                </div>
                                                <% }); } } } %>

                                            </div>

                                            <div class="clearfix"></div>

                                            <% if (page.comments) { %>
                                            <%- page.comments %>
                                            <% } %>

                                            <div class="clearfix"></div>

                                        </div>

                                        <div id="right">

                                            <% include includes/right.ejs %>

                                        </div>

                                        <div class="clear"></div>

                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                </div>

                <div class="recentlyViewedBlock" style="margin: 25px; display: none;">
                    <div><%- page.l.watched %>:</div>
                    <div id="recentlyViewed"></div>
                </div>

                <div id="spacer"></div>

            </div>

            <div id="footer">

                <% include includes/footer.ejs %>

            </div>

        </div>

    </div>

</div>

</body>
</html>