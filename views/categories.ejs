<!doctype html>
<html lang="ru">
<head>

    <% include includes/head.ejs %>

</head>
<body class="class_body">

<div class="new_top_menu" id="new_top_menu">
    <div class="line">
        <a href="/" rel="nofollow" class="logo"></a>
        <div class="right">
            <div class="search">
                <form action="/<%- page.urls.search %>/title" name="searchform" method="get">
                    <input type="text" class="search_inp" name="q" autocomplete="off" placeholder="Поиск на сайте..."/>
                    <input type="submit" class="search_but" value="" rel="nofollow"/>
                </form>
            </div>
            <% page.urls.years.slice(0,1).forEach(function(year) { %>
            <div class="login">
                <a href="<%- year.url %>" rel="nofollow" class="new_films"></a>
            </div>
            <% }); %>
        </div>
    </div>
</div>

<div id="hv2_1">

    <div class="brand_back"></div>
    <div class="brand_back_wrap"></div>

    <div id="hv2_2">

        <div id="hv2_4">

            <div id="wrapper">

                <div id="header">

                    <% include includes/header.ejs %>

                </div>

                <div id="container">

                    <div class="con1 ">
                        <div class="con2">
                            <div class="con3">
                                <div class="con4">
                                    <div class="con5">

                                        <div id="left">

                                            <% include includes/left.ejs %>

                                        </div>

                                        <div id="center" class="aviable_cat">

                                            <div class="bl_seo_txt_h bl_seo_txt_h_categ_sb">
                                                <div class="bl_seo_txt_bl_2">
                                                    <div class="tit_speedbar">
                                                        <span id="cinemapress-speedbar">
                                                            <span>
                                                                <a href="/" rel="nofollow">
                                                                    Главная
                                                                </a>
                                                            </span>
                                                            <span class="separ"></span>
                                                            <span>
                                                                <%= page.title %>
                                                            </span>
                                                        </span>
                                                    </div>
                                                </div>
                                            </div>

                                            <% if (page.seo) { %>
                                            <div class="bottom_descr">
                                                <div class="bl_seo_txt bl_seo_txt_txt">
                                                    <%- page.seo %>
                                                </div>
                                                <div class="clear"></div>
                                            </div>
                                            <% } %>

                                            <div id="cinemapress-content">

                                                <div class="sh0">
                                                    <div class="sh1">
                                                        <div class="sh2">
                                                            <h1>
                                                                <%= page.title %>
                                                            </h1>
                                                        </div>
                                                    </div>

                                                    <% if (typeof categories === 'object' && categories.length) { %>
                                                    <div style="clear: both"></div>
                                                    <% categories.forEach(function (content) { %>
                                                    <div class="cinemapress-news-block">
                                                        <div style="margin: 0; padding: 10px">
                                                            <a href="<%- content.url %>" class="cinemapress-news">
                                                                <img src="<%- content.image %>" class="cinemapress-news-image" alt="<%= content.title %>" style="height: 160px">
                                                                <div class="cinemapress-news-title">
                                                                    <%= content.title %>
                                                                </div>
                                                            </a>
                                                        </div>
                                                    </div>
                                                    <% }); %>
                                                    <div style="clear: both"></div>
                                                    <% } %>

                                                </div>

                                            </div>

                                        </div>

                                        <div id="right">

                                            <% include includes/right.ejs %>

                                        </div>

                                        <div class="clear"></div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                </div>

                <div class="recentlyViewedBlock" style="margin: 25px; display: none;">
                    <div>Вы надавно смотрели:</div>
                    <div id="recentlyViewed"></div>
                </div>

                <div id="spacer"></div>

            </div>

            <div id="footer">

                <% include includes/footer.ejs %>

            </div>

        </div>

    </div>

</div>

</body>
</html>